<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FindPartner">

    <Button
        android:id="@+id/backButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="41dp"
        android:background="@android:color/holo_purple"
        android:text="Back"
        app:layout_constraintStart_toStartOf="@+id/include"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/option9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:background="#070707"
        android:text="more"
        android:textColor="#FFFFFF"
        app:layout_constraintBaseline_toBaselineOf="@+id/option8"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/option8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="164dp"
        android:background="#070707"
        android:text="american"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/option9" />

    <Button
        android:id="@+id/option7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:background="#070707"
        android:text="breakfast"
        android:textColor="#FFFFFF"
        app:layout_constraintBaseline_toBaselineOf="@+id/option8"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/option6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:background="#070707"
        android:text="dessert"
        android:textColor="#FFFFFF"
        app:layout_constraintBaseline_toBaselineOf="@+id/option5"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/option5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:background="#070707"
        android:text="mexican"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/option6"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/option4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:background="#070707"
        android:text="sandwiches"
        android:textColor="#FFFFFF"
        app:layout_constraintBaseline_toBaselineOf="@+id/option5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/option3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:background="#070707"
        android:text="Indian"
        android:textColor="#FFFFFF"
        app:layout_constraintBaseline_toBaselineOf="@+id/option2"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/option2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:background="#070707"
        android:text="CHINESE"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/option3"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <include
        android:id="@+id/include"
        layout="@layout/actionbar"
        android:layout_width="364dp"
        android:layout_height="67dp"
        android:background="@android:color/holo_purple"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="14dp"
        android:text="Find a Partner"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/backButton2"
        app:layout_constraintTop_toTopOf="@+id/include" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:text="What are you in the mood for?"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/include"
        app:layout_constraintTop_toBottomOf="@+id/backButton2" />

    <Button
        android:id="@+id/option1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:background="#070707"
        android:text="Italian"
        android:textColor="#FFFFFF"
        app:layout_constraintBaseline_toBaselineOf="@+id/option2"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>